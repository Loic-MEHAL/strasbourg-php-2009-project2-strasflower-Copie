<div class="mt-5" id="formular">
  <form action="/User/add" method="POST">
    <fieldset>
      <legend class="text-center">Formulaire Inscription</legend>
      <p class="text-center">
        <label for="email">Email</label>
        <input id="email" type="email" name="email" placeholder="hackermanjean@gmail.com" value="">
      </p>
      <p class="text-center">
        <label for="nom">Nom</label>
        <input id="nom" type="text" name="nom" placeholder="Hackerman" value="">
      </p>
      <p class="text-center">
        <label for="prenom">Prenom</label>
        <input id="prenom" type="text" name="prenom" placeholder="Jacky" value="">
      </p>
      <p class="text-center">
        <label for="password">Mot de passe</label>
        <input id="password" type="password" name="password" placeholder="********" value="">
      </p>
      <p class="text-center">
        <label for="adress">Adresse</label>
        <input id="adress" type="text" name="adress" placeholder="1 rue de paris 75 001 Paris" value="">
      </p>
      <p class="text-center">
        <label for="age">Age</label>
        <input id="age" type="text" name="age" placeholder="35" value="">
      </p>
      <p>
        <div class="d-flex align-items-center">
          <label class="text-center mt-3 mb-0" for="sexe">Identit√© de genre</label>
        </div>
        <div class="d-flex align-items-center">
          <select id="sexe1" type="select" name="sexe" value="">
            <option>Homme</option>
            <option>Femme</option>
          </select>
        </div>
      </p>
      <button type="submit" class="btn btn-primary">Valider l'inscription</button>
    </fieldset>
  </form>
</div>
